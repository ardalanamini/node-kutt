{
  "name": "kutt",
  "version": "2.0.0",
  "description": "Node.js & browser (TypeScript) client for Kutt url shortener",
  "author": "Ardalan Amini <ardalanamini22@gmail.com> (https://ardalanamini.com)",
  "license": "MIT",
  "homepage": "https://kutt.it",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ardalanamini/node-kutt.git"
  },
  "bugs": {
    "url": "https://github.com/ardalanamini/node-kutt/issues"
  },
  "keywords": [
    "kutt",
    "url",
    "shortener"
  ],
  "sideEffects": false,
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "module": "es/index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "default": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "build:es": "tsc -p tsconfig.es.json",
    "build:hybrid": "scripts/pre-build.sh && npm run build && npm run build:es && scripts/post-build.sh",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix"
  },
  "peerDependencies": {
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.18.0",
    "axios": "^0.26.1",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.26.0",
    "typescript": "^4.4.4"
  }
}
